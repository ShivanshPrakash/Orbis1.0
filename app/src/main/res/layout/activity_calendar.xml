<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="shivansh.orbis10.Calendar"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageButton
        android:id="@+id/Button1"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/button1"
        app:layout_constraintRight_toLeftOf="@+id/Button2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <ImageButton
        android:id="@+id/Button2"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/button2"
        app:layout_constraintRight_toLeftOf="@+id/Button3"
        app:layout_constraintLeft_toRightOf="@+id/Button1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <ImageButton
        android:id="@+id/Button3"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/button3"
        app:layout_constraintRight_toLeftOf="@+id/Button4"
        app:layout_constraintLeft_toRightOf="@+id/Button2"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <ImageButton
        android:id="@+id/Button4"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/button4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Button3"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" />

    <ImageButton
        android:id="@+id/Button5"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/button5"
        app:layout_constraintRight_toLeftOf="@+id/Button6"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/Button1" />

    <ImageButton
        android:id="@+id/Button6"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/button6"
        app:layout_constraintRight_toLeftOf="@+id/Button7"
        app:layout_constraintLeft_toRightOf="@+id/Button5"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/Button2" />

    <ImageButton
        android:id="@+id/Button7"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/button7"
        app:layout_constraintRight_toLeftOf="@+id/Button8"
        app:layout_constraintLeft_toRightOf="@+id/Button6"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/Button3" />

    <ImageButton
        android:id="@+id/Button8"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/button8"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/Button7"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/Button4" />

    <ImageButton
        android:id="@+id/Button9"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/button9"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/Button5" />

    <ImageButton
        android:id="@+id/settingsButton"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/settings"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp" />

    <TextView
        android:id="@+id/displayData"
        android:layout_width="100dp"
        android:layout_height="46dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:text="Set start and end date"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <ImageButton
        android:id="@+id/Button10"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/day10"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.531" />

</android.support.constraint.ConstraintLayout>
